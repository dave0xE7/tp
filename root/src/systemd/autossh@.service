[Unit]
Description=autossh: %I
Documentation=file:///usr/share/doc/autossh/README.service man:autossh(1)
After=network.target

[Service]
User=autossh
Environment="AUTOSSH_GATETIME=0"
#EnvironmentFile=/opt/plug/autossh/%i.conf
EnvironmentFile=/opt/plug/autossh/1.conf
ExecStart=/usr/bin/autossh $OPTIONS -R %i:localhost:22

[Install]
WantedBy=multi-user.target

